/***************** Struct **************************/

struct pokemon {
	char nombre[7];
	int vida;
	int atk;
	int def;
};

/****************** Function Declaration ****************/
static void TMR_Music_Handler(void *baseaddr_p);
static int InterruptSystemSetup(XScuGic *XScuGicInstancePtr);
static int IntcInitFunction(u16 DeviceId, XTmrCtr *TmrInstancePtr);
void init_music(int *dir);

/*************** Function Def *************************/
//Interrupts
int IntcInitFunction(u16 DeviceId, XTmrCtr *TmrInstancePtr)
{
	XScuGic_Config *IntcConfig;
	int status;

	IntcConfig = XScuGic_LookupConfig(DeviceId);
	status = XScuGic_CfgInitialize(&INTC, IntcConfig, IntcConfig->CpuBaseAddress);
	if(status != XST_SUCCESS) return XST_FAILURE;

	status = InterruptSystemSetup(&INTC);
	if(status != XST_SUCCESS) return XST_FAILURE;

	XScuGic_SetPriorityTriggerType(&INTC, INTC_TMR_INTERRUPT_ID, 0x18, 0x1);

	status = XScuGic_Connect(&INTC,INTC_TMR_INTERRUPT_ID,(Xil_ExceptionHandler)TMR_Music_Handler,(void *) TmrInstancePtr);
	if(status != XST_SUCCESS) return XST_FAILURE;

	XScuGic_Enable(&INTC, INTC_TMR_INTERRUPT_ID);

	return XST_SUCCESS;
}

int InterruptSystemSetup(XScuGic *XScuGicInstancePtr)
{

	Xil_ExceptionRegisterHandler(XIL_EXCEPTION_ID_INT,
	(Xil_ExceptionHandler)XScuGic_InterruptHandler,
	XScuGicInstancePtr);

	Xil_ExceptionEnable();

	return XST_SUCCESS;

}

void TMR_Music_Handler(void *data)
{
	//xil_printf("frecuencia enviada %d \n\r", frecuencias[counter1]);
	Xil_Out32(dir_frecuencia , frecuencias[counter1]);
	if (counter1 <= 3472){
		counter1++;

	}
	else{
		counter1 = 0;
	}

	XTmrCtr_Stop(data,TIMER_CNTR_0);
	XTmrCtr_Reset(data,TIMER_CNTR_0);
	XTmrCtr_Start(data,TIMER_CNTR_0);

}

//Estados

void inicio(){
	xil_printf("Bienvenido a SEPmon \n\r", frecuencias[counter1]);
	GUI(INICIO);
}

int atacar(struct pokemon* pkm1, struct pokemon* pkm2)
{
	xil_printf("Que deberia hacer %c \n\r", pkm1->nombre);

	int ataque;
	xil_printf("1. Alarido \n\r");
	xil_printf("2. Terremoto \n\r");
	xil_printf("3. Noche oscura \n\r");
	xil_printf("4. Ataque rapido \n\r");


	scanf("%d",ataque);


	return 50;

}

int batalla(struct pokemon* pkm1, struct pokemon* pkm2){
	int dano1 = 0;
	xil_printf("Te haz encontrado con un %c \n\r", pkm2->nombre);
	xil_printf("Ve %c \n\r", *pkm1->nombre);
	int continua = 1;
	int turno = 1;
	while (continua){
		if (pkm1->vida <= 0){
			pkm1->vida = 0;
			continua = 0;
			return 0;
			break;

		}
		if (pkm2->vida <= 0){
			pkm2->vida = 0;
			continua = 0;
			return 1;
			break;

		}
		if (turno){
			dano1 = atacar(pkm1, pkm2);
			pkm2->vida -= dano1;
			xil_printf("%c ha recibido %d de daño \n\r", *pkm2->nombre, dano1);
			turno  = 0;
		}
		else{
			dano1 = 30;
			xil_printf("%c ha recibido %d de daño \n\r", *pkm1->nombre, dano1);
			pkm1->vida -= dano1;
			turno = 1;
		}

	}
	return 1;
}

void fin(int resultado){
	if (resultado){
		xil_printf("Felicidades ganaste!!!");
	}
	else{
		xil_printf("Lastima :(, para la proxima sera.");
	}
}

// Generales

void init_music(int *dir) // preguntar a que se refiere con que reciba como argumento un puntero
{
	xil_printf("Ingrese el volumen deseado:\n\r");
	scanf("%d",dir);
	Xil_Out32(dir_volumen, *dir);
	xil_printf("Se a ingresado el volumen: %d\n\r", *dir);
}


int init(){
	int Status;
	//Initialize the UART
	init_platform();
	xil_printf("Iniciando...\n\r");

	int vol = 0;
	int *dir_vol = &vol;

	init_music(dir_vol);

	/************ Iniciar interrupciones *********************/

	Status = XTmrCtr_Initialize(&TmrMusic, TMR_DEVICE_ID);

	if(Status != XST_SUCCESS) return XST_FAILURE;


	XTmrCtr_SetHandler(&TmrMusic, (XTmrCtr_Handler) TMR_Music_Handler, &TmrMusic);
	XTmrCtr_SetResetValue(&TmrMusic, TIMER_CNTR_0, MUSIC_RESET);
	XTmrCtr_SetOptions(&TmrMusic, TIMER_CNTR_0, XTC_INT_MODE_OPTION | XTC_AUTO_RELOAD_OPTION );


	Status = IntcInitFunction(INTC_DEVICE_ID, &TmrMusic);

	if(Status != XST_SUCCESS) return XST_FAILURE;

	XTmrCtr_Start(&TmrMusic, TIMER_CNTR_0);

	/* Initialize the GPIO 0 driver */
	Status = XGpio_Initialize(&gpio0, XPAR_AXI_GPIO_0_DEVICE_ID);
	if (Status != XST_SUCCESS) {
		xil_printf("Gpio 0 Initialization Failed\r\n");
		return XST_FAILURE;
	}

	// Set up the AXI SPI Controller 0 (Screen)
	Status = XSpi_Init(&SpiInstance,SPI_DEVICE_ID);
	if (Status != XST_SUCCESS) {
		xil_printf("SPI Mode Failed\r\n");
		return XST_FAILURE;
	}
	// Set up the AXI SPI Controller 0 (Joystick(x,y), accelerometer, potentiometer, mic)
	Status = init_adc(&SpiInstance1, SPI_DEVICE_ID_1);
	if (Status != XST_SUCCESS) {
		xil_printf("SPI-ADC Mode Failed\r\n");
		return XST_FAILURE;
	}
	// Set up the AXI IIC Controller 0 (temperature sensor, light sensor)
	Status = init_IIC();
	if (Status != XST_SUCCESS) {
		xil_printf("IIC Mode Failed\r\n");
		return XST_FAILURE;
	}
	//Write through UART to PC
	xil_printf("TFT initialized \r\n");
	xil_printf("**********Init LCD**********\r\n");
	// Init screen
	LCD_SCAN_DIR LCD_ScanDir = SCAN_DIR_DFT;//SCAN_DIR_DFT = D2U_L2R
	LCD_Init(LCD_ScanDir );
	// Default intro image from screen company
	xil_printf("LCD Show \r\n");

	GUI_Show();
	delay_ms(500);
	LCD_Clear(GUI_BACKGROUND);

	// Default intro image given by SEP course.
	GUI_INTRO();
	delay_ms(500);
	LCD_Clear(BACKGROUND);

	print_image();
	return Status;
}


int main()
{
	int Status;




	struct pokemon gengar;
	strcpy(gengar.nombre, "gengar");
	gengar.vida = 100;
	gengar.atk = 100;
	gengar.def = 100;

	struct pokemon *pgengar = &gengar;



	struct pokemon nidoran;
	strcpy(nidoran.nombre, "nidoran");
	nidoran.vida = 100;
	nidoran.atk = 100;
	nidoran.def = 100;

	struct pokemon *pnidoran = &nidoran;


	init_platform();

	Status = init();
	if (Status != XST_SUCCESS) {
		xil_printf("Init Failed\r\n");
		return XST_FAILURE;
	}



	int resultado = 0;

	inicio();


	resultado = batalla(pgengar, pnidoran);
	fin(resultado);

	//Define arrays for joystick, accelerometer, temperatura and ligth sensors.
	// The ADC transform the data in 10bits, for temperatura and light sensors the data lenght is 16bits. For simplicity all
	// arrays are defined as 16bits.

	while(1){
	}
    return 0;
}
